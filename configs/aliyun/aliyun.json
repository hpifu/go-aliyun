{
    "service": {
        "port": ":7060",
        "allowOrigins": [
            "https://aliyun.hatlonely.com"
        ],
        "cookieSecure": true,
        "cookieDomain": "aliyun.hatlonely.com"
    },
    "store": {
        "parameter": {
            "root": "data/parameter"
        },
        "credential": {
            "root": "data/credential"
        }
    },
    "logger": {
        "info": {
            "formatter": "text",
            "filename": "log/aliyun.info",
            "maxAge": "24h"
        },
        "warn": {
            "formatter": "text",
            "filename": "log/aliyun.warn",
            "maxAge": "24h"
        },
        "access": {
            "formatter": "json",
            "filename": "log/aliyun.access",
            "maxAge": "24h"
        }
    }
}